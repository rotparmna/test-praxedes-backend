{
  "IdentityUrl": "https://localhost:5001",
  "EndpointJsonPlaceholder": "https://jsonplaceholder.typicode.com",
  "PathJsonPlaceholderPost": "/posts",
  "PathJsonPlaceholderComment": "/comments",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Error",
        "System": "Error",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  }
}

